<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">Arcade score table</div>
            <div class="panel-body">
                <table class="table table-striped">
                    <tr>
                        <th>#</th>
                        <th>Name</th>
                        <th>Time</th>
                        <th>Score</th>
                    </tr>
                    <% _.each(scores, function (score) { %>
                        <tr <%= score.position === 1 ? 'class=success' : '' %>>
                            <td> <%= score.position %></td>
                            <td><%= score.playerName %></td>
                            <td class="text-right"><%= score.time %> sec</td>
                            <td class="text-right"><%= score.score %></td>
                        </tr>
                    <% }) %>
                </table>
            </div>
        </div>
    </div>
</div>